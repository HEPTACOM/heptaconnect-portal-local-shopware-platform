{
    "name": "heptacom/heptaconnect-portal-local-shopware-platform",
    "description": "HEPTAconnect portal to connect a local running shopware platform",
    "license": [
        "AGPL-3.0-or-later",
        "proprietary"
    ],
    "type": "library",
    "version": "0.8.1",
    "keywords": [
        "heptaconnect-portal"
    ],
    "authors": [
        {
            "name": "HEPTACOM GmbH"
        }
    ],
    "support": {
        "issues": "https://github.com/HEPTACOM/heptaconnect-portal-local-shopware-platform/issues",
        "source": "https://github.com/HEPTACOM/heptaconnect-portal-local-shopware-platform",
        "docs": "https://connect.heptacom.de/",
        "rss": "https://connect.heptacom.de/feed/rss2.xml"
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "bentools/iterable-functions": "^1.4",
        "heptacom/heptaconnect-bridge-shopware-platform": "^0.8",
        "heptacom/heptaconnect-core": "^0.8",
        "heptacom/heptaconnect-dataset-base": "^0.8",
        "heptacom/heptaconnect-dataset-ecommerce": "^0.8",
        "heptacom/heptaconnect-portal-base": "^0.8",
        "heptacom/heptaconnect-storage-base": "^0.8",
        "psr/container": "^1",
        "psr/http-message": "^1",
        "ramsey/uuid": "^3.5 || ^4",
        "shopware/core": ">=6.2.1",
        "symfony/dependency-injection": "^4.4 || ^5.0",
        "symfony/http-foundation": "^4.4 || ^5.0",
        "symfony/mime": "^4.4 || ^5.0"
    },
    "require-dev": {
        "infection/infection": ">=0.21",
        "phpunit/phpunit": ">=9.3 <10"
    },
    "conflict": {
        "nikic/php-parser": "4.7.0"
    },
    "repositories": {
        "heptaconnect-sources": {
            "type": "path",
            "url": "../**"
        }
    },
    "autoload": {
        "psr-4": {
            "Heptacom\\HeptaConnect\\Portal\\LocalShopwarePlatform\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Heptacom\\HeptaConnect\\Portal\\LocalShopwarePlatform\\Test\\": "test/"
        }
    },
    "extra": {
        "heptaconnect": {
            "portals": [
                "Heptacom\\HeptaConnect\\Portal\\LocalShopwarePlatform\\Portal"
            ]
        },
        "unused": [
            "heptacom/heptaconnect-bridge-shopware-platform"
        ]
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    },
    "website": "https://heptaconnect.io"
}
