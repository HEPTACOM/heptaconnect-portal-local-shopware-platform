{
    "name": "heptacom/heptaconnect-portal-local-shopware-platform",
    "type": "library",
    "description": "HEPTAconnect portal to connect a local running shopware platform",
    "keywords": [
        "heptaconnect-portal"
    ],
    "license": [
        "AGPL-3.0-or-later",
        "proprietary"
    ],
    "version": "0.3.1",
    "authors": [
        {
            "name": "HEPTACOM GmbH"
        }
    ],
    "require": {
        "php": ">=7.4",
        "heptacom/heptaconnect-bridge-shopware-platform": "self.version",
        "heptacom/heptaconnect-core": "self.version",
        "heptacom/heptaconnect-dataset-ecommerce": "self.version",
        "heptacom/heptaconnect-portal-base": "self.version",
        "shopware/core": ">=6.2.1"
    },
    "conflict": {
        "nikic/php-parser": "4.7.0"
    },
    "require-dev": {
        "dq5studios/psalm-junit": ">=2",
        "ergebnis/composer-normalize": ">=2.7",
        "friendsofphp/php-cs-fixer": ">=2.17",
        "icanhazstring/composer-unused": ">=0.7",
        "infection/infection": ">=0.21",
        "maglnet/composer-require-checker": ">=2",
        "mavimo/phpstan-junit": ">=0.3",
        "phpstan/extension-installer": ">=1",
        "phpstan/phpstan": ">=0.12",
        "phpstan/phpstan-deprecation-rules": ">=0.12",
        "phpstan/phpstan-phpunit": ">=0.12",
        "phpstan/phpstan-strict-rules": ">=0.12",
        "phpunit/phpunit": ">=9.3 <10",
        "psalm/plugin-phpunit": ">=0.10",
        "vimeo/psalm": ">=4"
    },
    "autoload": {
        "psr-4": {
            "Heptacom\\HeptaConnect\\Portal\\LocalShopwarePlatform\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Heptacom\\HeptaConnect\\Portal\\LocalShopwarePlatform\\Test\\": "test/"
        }
    },
    "repositories": {
        "heptaconnect-sources": {
            "type": "path",
            "url": "../**"
        }
    },
    "support": {
        "issues": "https://github.com/HEPTACOM/heptaconnect-portal-local-shopware-platform/issues",
        "source": "https://github.com/HEPTACOM/heptaconnect-portal-local-shopware-platform",
        "docs": "https://connect.heptacom.de/",
        "rss": "https://connect.heptacom.de/feed/rss2.xml"
    },
    "extra": {
        "heptaconnect": {
            "portals": [
                "Heptacom\\HeptaConnect\\Portal\\LocalShopwarePlatform\\Portal"
            ]
        }
    }
}
