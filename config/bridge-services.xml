<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="Heptacom\HeptaConnect\Portal\LocalShopwarePlatform\Core\Portal\PortalFactory"
                 decorates="Heptacom\HeptaConnect\Core\Portal\PortalFactory">
            <argument type="service" id="Heptacom\HeptaConnect\Portal\LocalShopwarePlatform\Core\Portal\PortalFactory.inner"/>
            <argument type="service" id="Heptacom\HeptaConnect\Portal\LocalShopwarePlatform\Portal"/>
        </service>

        <service id="Heptacom\HeptaConnect\Portal\LocalShopwarePlatform\Portal">
            <argument type="service" id="request_stack"/>
            <argument type="service" id="Heptacom\HeptaConnect\Core\Storage\NormalizationRegistry"/>
            <argument type="service" id="Shopware\Core\Content\Media\File\FileSaver"/>
            <argument type="service" id="Shopware\Core\Content\Media\MediaService"/>
            <argument type="service" id="service_container"/>
            <argument type="service" id="Shopware\Core\System\StateMachine\StateMachineRegistry"/>
            <argument type="service" id="Shopware\Core\Framework\DataAbstractionLayer\DefinitionInstanceRegistry"/>
        </service>
    </services>
</container>
